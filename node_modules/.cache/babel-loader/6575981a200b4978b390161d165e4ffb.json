 :height=\"height\" radius=\"2px\" :class=\"$style.quickBtn\" @click.stop=\"openSet\"></Button>\r\n        <table :class=\"$style.timeSelectBox\" :style=\"{borderColor:theme.border_color_dark}\" v-show=\"timeSelectShow\">\r\n          <tr>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick(1)\">\r\n              <i class=\"el-icon-date\" style=\"margin:0 5px 0 3px;\"></i>今天\r\n            </td>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick(3)\">近3天</td>\r\n          </tr>\r\n          <tr>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick('yesterday')\">\r\n              <i class=\"el-icon-date\" style=\"margin:0 5px 0 3px;\"></i>昨天\r\n            </td>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick(5)\">近5天</td>\r\n          </tr>\r\n          <tr>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick('tweek')\">\r\n              <i class=\"el-icon-date\" style=\"margin:0 5px 0 3px;\"></i>本周\r\n            </td>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick(7)\">近7天</td>\r\n          </tr>\r\n          <tr>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick('lweek')\">\r\n              <i class=\"el-icon-date\" style=\"margin:0 5px 0 3px;\"></i>上周\r\n            </td>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick(10)\">近10天</td>\r\n          </tr>\r\n          <tr>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick('tmonth')\">\r\n              <i class=\"el-icon-date\" style=\"margin:0 5px 0 3px;\"></i>本月\r\n            </td>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick(15)\">近15天</td>\r\n          </tr>\r\n          <tr>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick('lmonth')\">\r\n              <i class=\"el-icon-date\" style=\"margin:0 5px 0 3px;\"></i>上月\r\n            </td>\r\n            <td :style=\"{borderColor:theme.border_color_dark}\" @click.stop=\"timeSelectClick(30)\">近30天</td>\r\n          </tr>\r\n        </table>\r\n    </div>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { addDateTime, getNowDate ,getDays , dateFormat} from '@/common/util/Dates.js';\r\n\r\nexport default {\r\n  name: \"ButtonTime\",\r\n  props:{height:{ type: Number},timeType:{type: String,default:'day'}},//timeType:'day','HMS','HM'\r\n  data() {\r\n    return {\r\n      sdate:null,\r\n      edate:null,\r\n      timeSelectShow: false,\r\n    };\r\n  },\r\n  methods: {\r\n    timeSelectClick(time) {\r\n      let Sdate,Edate;\r\n      switch (this.timeType) {\r\n        case 'day':\r\n          Sdate='';\r\n          Edate='';\r\n          break;\r\n        case 'HMS':\r\n          Sdate=' 00:00:00';\r\n          Edate=' 23:59:59';\r\n          break;\r\n        case 'HM':\r\n          Sdate=' 00:00';\r\n          Edate=' 23:59';\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n      if(time == 'yesterday'){\r\n        this.sdate = addDateTime(-1, 'YYYY-MM-DD') + Sdate;\r\n        this.edate = addDateTime(-1, 'YYYY-MM-DD') + Edate;\r\n      }else if(time == 'tweek'){\r\n        this.sdate = this.getTime(0) + Sdate;\r\n        this.edate = getNowDate() + Edate;\r\n      }else if(time == 'lweek'){\r\n        this.sdate = this.getTime(7) + Sdate;\r\n        this.edate = this.getTime(1) + Edate;\r\n      }else if(time == 'tmonth'){\r\n        this.sdate = this.getMonthTime(0) + Sdate;\r\n        this.edate = getNowDate() + Edate;\r\n      }else if(time == 'lmonth'){\r\n        this.sdate = this.getMonthTime(1) + Sdate;\r\n        this.edate = this.getMonthTime(1, true) + Edate;\r\n      }else{\r\n        this.sdate = addDateTime(-(time-1), 'YYYY-MM-DD') + Sdate;\r\n        this.edate = getNowDate() + Edate;\r\n      }\r\n      this.$emit('func',this.sdate,this.edate)\r\n      this.timeSelectShow = false;\r\n    },\r\n    getTime(n) {\r\n      var now = new Date();\r\n      var year = now.getFullYear();\r\n      //因为月份是从0开始的,所以获取这个月的月份数要加1才行\r\n      var month = now.getMonth() + 1;\r\n      var date = now.getDate();\r\n      var day = now.getDay();\r\n      //判断是否为周日,如果不是的话,就让今天的day-1(例如星期二就是2-1)\r\n      if (day !== 0) {\r\n        n = n + (day - 1);\r\n      } else {\r\n        n = n + day;\r\n      }\r\n      if (day) {\r\n        //这个判断是为了解决跨年的问题\r\n        if (month > 1) {\r\n          month = month;\r\n        }\r\n        //这个判断是为了解决跨年的问题,月份是从0开始的\r\n        else {\r\n          year = year - 1;\r\n          month = 12;\r\n        }\r\n      }\r\n      now.setDate(now.getDate() - n);\r\n      return dateFormat(now);\r\n    },\r\n    getMonthTime(month, end = false) {\r\n      var now = new Date();\r\n      now.setMonth(now.getMonth() - month);\r\n      var myYear = now.getFullYear();\r\n      var myMonth = now.getMonth();\r\n      var monthStartDate = new Date(myYear, myMonth, 1);\r\n      if (end) {\r\n        monthStartDate = new Date(\r\n          myYear,\r\n          myMonth,\r\n          getDays(myYear, myMonth + 1)\r\n        );\r\n      }\r\n      return dateFormat(monthStartDate);\r\n    },\r\n    openSet() {this.timeSelectShow ? this.hide() : this.show()},\r\n    show () {\r\n      this.timeSelectShow = true\r\n      document.addEventListener('click', this.hidePanel, false)\r\n    },\r\n    hide () {\r\n      this.timeSelectShow = false\r\n      document.removeEventListener('click', this.hidePanel, false)\r\n    },\r\n    hidePanel (event) { if(event.target.id!='setTime'){this.hide()}},\r\n  }\r\n};\r\n</script>\r\n<style module>\r\n.content{\r\n    position: relative;\r\n}\r\n.quickBtn {\r\n  height: 100%;\r\n  width: 25px;\r\n  margin-left: 10px;\r\n  padding: 0px 0px !important;\r\n}\r\n.timeSelectBox {\r\n  position: fixed;\r\n  margin-left: 34px;\r\n  border: 1px solid;\r\n  width: 125px;\r\n  height: 135px;\r\n  z-index: 9999;\r\n  border-collapse: collapse;\r\n}\r\n.timeSelectBox td {\r\n  background-color: #fff;\r\n  border-left: 1px solid;\r\n  cursor: default;\r\n}\r\n.timeSelectBox td:hover {\r\n  background-color: #fcf0c1;\r\n}\r\n.timeSelectBox tr td:nth-child(2) {\r\n  background-image: url(\"@/components/Tree/img/leaf.png\");\r\n  background-repeat: no-repeat;\r\n  background-position: 3px 50%;\r\n  padding-left: 22px;\r\n  width: 68px;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}