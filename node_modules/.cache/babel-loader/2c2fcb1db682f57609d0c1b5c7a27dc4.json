=\"containerStyle\" @mouseenter=\"hovering=true\" @mouseleave=\"hovering=false\">\r\n\r\n    <!--label显示-->\r\n    <span :class=\"$style.label\" :style=\"labelStyle\" v-if=\"label\" is-label>{{label}}</span>\r\n\r\n    <!--必填显示-->\r\n    <span :class=\"[$style.required,'el-icon-star-on']\" :style=\"{backgroundColor:theme.input_label_bg_color}\" v-if=\"required\"></span>\r\n\r\n    <!--单位显示-->\r\n    <span :class=\"$style.unit\" v-if=\"unit\">{{unit}}</span>\r\n\r\n    <!--clear按钮显示-->\r\n    <div :class=\"$style.clear\" v-if=\"showClear\">\r\n      <i></i>\r\n      <i @click=\"clear\" class=\"el-icon-close\"></i>\r\n    </div>\r\n\r\n    <!--input显示-->\r\n    <div style=\"overflow:hidden;\">\r\n      <textarea ref=\"textarea\" \r\n        :class=\"$style.textarea\" \r\n        :style=\"[inputStyle, {width:textareaWidth}]\"\r\n        v-bind=\"$attrs\"\r\n        :value=\"value\"\r\n        :maxlength=\"maxlength\"\r\n        :disabled=\"disabled\"\r\n        :readonly=\"readonly\"\r\n        :autocomplete=\"autocomplete\"\r\n        @click=\"handleClick\"\r\n        @compositionstart=\"handleCompositionStart\"\r\n        @compositionend=\"handleCompositionEnd\"\r\n        @input=\"handleTextarea\"\r\n        @focus=\"handleFocus\"\r\n        @blur=\"handleBlur\"\r\n        @change=\"handleChange\">\r\n      </textarea>\r\n    </div >\r\n\r\n    <div v-if=\"disabled\" :class=\"$style.disabled\" :style=\"{borderRadius: radius}\"/>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  /*\r\n  * 使用例子\r\n  * <InputTextArea label=\"你想输入啥\" v-model=\"text\" :value=\"内容\" @change=\"onTextChg\" clearable required :height=\"50\"/>\r\n  * \r\n  *  data: {text: ''},\r\n  *  methods: {\r\n  *    onTextChg(text) {\r\n  *    }\r\n  *  }\r\n  */\r\n  \r\n  export default {\r\n    name: \"InputTextArea\",\r\n    props: {\r\n      value: [String, Number],                                        // 当前值(v-model)\r\n      label: String,                                                  // 标题文本\